<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    android:key="@+id/pref_screen">
    <PreferenceCategory android:title="PREFERÊNCIAS RSS">
        <EditTextPreference
            android:key="rssfeed"
            android:dialogMessage="Digite o endereço do feed RSS"
            android:dialogTitle="Alterar endeço"
            android:negativeButtonText="Cancelar"
            android:positiveButtonText="Salvar"
            android:title="Endereço de Feed"
            android:defaultValue="@string/rssfeed">
        </EditTextPreference>
    </PreferenceCategory>
</PreferenceScreen>
